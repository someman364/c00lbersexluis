local function C_9()
local script = G2L["9"];
	local ogtext = script.Parent.Text
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Text = "Executed"
		wait(1)
		script.Parent.Text = ogtext
	end)
end;
