		cmds.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		cmds.BorderColor3 = Color3.fromRGB(0, 0, 0)
		cmds.BorderSizePixel = 0
		cmds.Position = UDim2.new(0.5, 0, 0.5, 0)
		cmds.Size = UDim2.new(0.0883912593, 0, 0.287559688, 0)
		cmds.Visible = false
	
